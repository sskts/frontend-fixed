{
  "name": "sskts-frontend",
  "version": "1.8.0",
  "private": true,
  "scripts": {
    "ssl": "node ./ssl.js",
    "check": "npm run tslint && nsp check",
    "snyk": "snyk auth && snyk test --ignore-policy",
    "start": "node ./index.js",
    "ui-test": "node ./test/uiTest.js",
    "css": "npm run scss && postcss -c ./.postcssrc.json",
    "scss": "node-sass public/scss --output public/css",
    "tslint": "tslint -c tslint.json app/**/**.ts libs/MP/**.ts --type-check --project tsconfig.json",
    "tslint-fix": "tslint -c tslint.json app/**/**.ts --project tsconfig.json --fix",
    "jsdoc": "grunt jsdoc",
    "clean": "rimraf libs/**/*.js app/**/*.js test/**/*.js docs/* npm-debug.log*",
    "build": "tsc",
    "test": "npm run check && npm run nyc",
    "preversion": "npm run clean && npm run build && npm run css && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "nyc": "nyc mocha --no-timeouts --recursive"
  },
  "dependencies": {
    "@motionpicture/coa-service": "^4.0.6",
    "@motionpicture/gmo-service": "^4.0.2",
    "@motionpicture/mvtk-service": "^2.0.2",
    "@motionpicture/sskts-api-nodejs-client": "^1.1.1",
    "basic-auth": "^1.1.0",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "ejs": "^2.5.6",
    "express": "^4.16.1",
    "express-ejs-layouts": "^2.3.0",
    "express-session": "^1.15.6",
    "express-validator": "^3.2.1",
    "fs-extra": "^4.0.2",
    "helmet": "^3.8.2",
    "http-status": "^1.0.1",
    "i18n": "^0.8.3",
    "moment": "^2.18.1",
    "uuid": "^3.1.0",
    "winston": "^2.4.0"
  },
  "main": "index.js",
  "devDependencies": {
    "@types/basic-auth": "0.0.31",
    "@types/body-parser": "^1.16.5",
    "@types/connect-redis": "0.0.5",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.29",
    "@types/email-templates": "0.0.32",
    "@types/express": "^4.0.37",
    "@types/express-session": "0.0.32",
    "@types/fs-extra": "^4.0.2",
    "@types/helmet": "0.0.33",
    "@types/http-status": "^0.2.30",
    "@types/i18n": "^0.5.20",
    "@types/mocha": "^2.2.43",
    "@types/redis": "^0.12.36",
    "@types/selenium-webdriver": "^2.53.39",
    "@types/sinon": "^2.3.5",
    "@types/supertest": "^2.0.3",
    "@types/uuid": "^3.4.1",
    "@types/winston": "^2.3.6",
    "autoprefixer": "^6.7.2",
    "cssnano": "^3.10.0",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-jsdoc": "^2.2.0",
    "ink-docstrap": "^1.3.0",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.3",
    "nsp": "^2.8.1",
    "nyc": "^11.2.1",
    "postcss": "^5.2.18",
    "postcss-cli": "^2.6.0",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^3.4.0",
    "sinon": "^3.3.0",
    "snyk": "^1.42.3",
    "supertest": "^3.0.0",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.3"
  },
  "repository": {},
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "name": "Akito Hataguchi",
      "email": "hataguchi@motionpicture.jp"
    },
    {
      "name": "Tetsu Yamazaki",
      "email": "yamazaki@motionpicture.jp"
    }
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.8"
  },
  "description": "",
  "nyc": {
    "check-coverage": true,
    "statements": 90,
    "branches": 80,
    "functions": 90,
    "lines": 90,
    "include": [
      "app/**/*.js"
    ],
    "exclude": [
      "app/forms/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
