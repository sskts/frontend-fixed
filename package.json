{
    "name": "sskts-frontend-fixed",
    "version": "2.16.2",
    "private": true,
    "scripts": {
        "dev": "node dist/ssl",
        "start": "node dist/index",
        "scss": "sass public/scss public/css",
        "lint": "tslint -c tslint.json src/**/**.ts  --project src/tsconfig.json",
        "lint:fix": "tslint -c tslint.json app/**/**.ts --project tsconfig.json --fix",
        "jsdoc": "grunt jsdoc",
        "clean": "rimraf src/**/*.js test/**/*.js docs/* npm-debug.log*",
        "build": "tsc --project ./src --locale ja-jp",
        "test": "npm run lint",
        "test:ui": "node ./test/uiTest.js",
        "preversion": "npm run clean && npm run build && npm test",
        "version": "git add -A",
        "postversion": "git push origin --tags",
        "nyc": "nyc mocha --no-timeouts --recursive",
        "deploy:development": "gcloud app deploy ./app.development.gcp.yaml --project sskts-dev",
        "deploy:test": "gcloud app deploy ./app.test.gcp.yaml --project sskts-test",
        "deploy:production": "gcloud app deploy ./app.production.gcp.yaml --project sskts-prod-frontend-fixed --no-promote --version staging"
    },
    "dependencies": {
        "@cinerino/sdk": "^5.1.0",
        "basic-auth": "^1.1.0",
        "connect-redis": "^5.0.0",
        "cookie-parser": "^1.4.3",
        "debug": "^2.6.9",
        "ejs": "^2.5.6",
        "express": "^4.17.1",
        "express-ejs-layouts": "^2.3.0",
        "express-session": "^1.17.0",
        "express-validator": "^3.2.1",
        "fs-extra": "^4.0.2",
        "google-libphonenumber": "^3.0.9",
        "helmet": "^3.9.0",
        "http-status": "^1.0.1",
        "i18n": "^0.8.3",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.37",
        "redis": "^3.1.2",
        "sinon": "^7.5.0",
        "uuid": "^3.1.0",
        "winston": "^2.4.0"
    },
    "main": "index.js",
    "devDependencies": {
        "@types/basic-auth": "0.0.31",
        "@types/connect-redis": "^0.0.23",
        "@types/cookie-parser": "^1.4.1",
        "@types/debug": "0.0.29",
        "@types/email-templates": "0.0.32",
        "@types/express": "4.17.17",
        "@types/express-session": "^1.17.0",
        "@types/fs-extra": "^4.0.5",
        "@types/google-libphonenumber": "^7.4.12",
        "@types/helmet": "0.0.33",
        "@types/http-status": "^0.2.30",
        "@types/i18n": "^0.5.20",
        "@types/mocha": "^2.2.44",
        "@types/node": "^18.18.13",
        "@types/redis": "^2.8.29",
        "@types/request": "^2.48.8",
        "@types/request-promise-native": "^1.0.18",
        "@types/selenium-webdriver": "^2.53.43",
        "@types/sinon": "^7.5.1",
        "@types/supertest": "^2.0.4",
        "@types/uuid": "^3.4.3",
        "@types/winston": "^2.3.7",
        "autoprefixer": "^9.7.2",
        "cssnano": "^4.1.10",
        "grunt": "^1.0.4",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-jsdoc": "^2.4.0",
        "ink-docstrap": "^1.3.2",
        "mocha": "^6.1.4",
        "npm-run-all": "^4.1.5",
        "rimraf": "^2.6.2",
        "sass": "^1.69.5",
        "selenium-webdriver": "^3.6.0",
        "snyk": "^1.251.0",
        "supertest": "^3.0.0",
        "tslint": "^6.1.3",
        "typescript": "4.9.5"
    },
    "repository": {},
    "author": {
        "name": "Motionpicture co.,ltd."
    },
    "contributors": [
        {
            "name": "Akito Hataguchi",
            "email": "hataguchi@motionpicture.jp"
        },
        {
            "name": "Tetsu Yamazaki",
            "email": "yamazaki@motionpicture.jp"
        }
    ],
    "license": "UNLICENSED",
    "description": "",
    "nyc": {
        "check-coverage": true,
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "include": [
            "app/**/*.js"
        ],
        "exclude": [
            "app/forms/**/*.js"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ]
    }
}
